<h1>Listing conceptual_fields</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Datatype</th>
    <th>Conceptual entity</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @conceptual_fields.each do |conceptual_field| %>
  <tr>
    <td><%=ConceptualEntity.find(conceptual_field.conceptual_entity_id).name %></td>
    <td><%= conceptual_field.name %></td>
    <td><%= conceptual_field.datatype %></td>
    <td><%= link_to 'Show', conceptual_field %></td>
    <td><%= link_to 'Edit', edit_conceptual_field_path(conceptual_field) %></td>
    <td><%= link_to 'Destroy', conceptual_field, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

